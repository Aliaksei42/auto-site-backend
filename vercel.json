{
    "functions": {
      "api/main.rs": {
        "runtime": "vercel-node@3.2.0",
        "excludeFiles": "node_modules/**/*"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/main"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  